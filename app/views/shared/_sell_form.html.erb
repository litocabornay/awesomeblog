<div class="row">
<div class="col-md-6 col-md-offset-3">
<!-- -- -->
<%= form_for (@sell) do |f| %>
<!--urlいらない　→　ただのリフレッシュだから-->
<!-- -- -->
<%= render 'shared/error_message', object: f.object  %>
<!-- -- -->
  <br>
  <div class="row">
    <div class="large-12 columns width100">
     <%= f.label :メーカー %>
     <br>
       <%#= f.collection_select :maker, Maker.all, :name, :name %>

       <%= f.select :maker, 
       [
       ["愛喜", "愛喜"], 
       ["アビリット", "アビリット"], 
       ["アムテックス", "アムテックス"],
       ["エース電研", "エース電研"],
       ["Aーgon", "Aーgon"],
       ["ＥＸＣＩＴＥ", "ＥＸＣＩＴＥ"],
       ["奥村", "奥村"],
       ["オッケー", "オッケー"],
       ["オリンピア", "オリンピア"],
       ["京楽", "京楽"],
       ["銀座", "銀座"],
       ["サミー", "サミー"],
       ["三共", "三共"],
       ["サンスリー", "サンスリー"],
       ["サンケイR＆D", "サンケイR＆D"],
       ["三洋", "三洋"],
       ["ジェイビー", "ジェイビー"],
       ["大一", "大一"],
       ["大都技研", "大都技研"],
       ["タイヨーエレック", "タイヨーエレック"],
       ["高尾", "高尾"],
       ["高砂電器産業", "高砂電器産業"],
       ["竹屋", "竹屋"],
       ["大和工業", "大和工業"],
       ["大和製作所", "大和製作所"],
       ["トビー", "トビー"],
       ["豊丸", "豊丸"],
       ["七匠", "七匠"],
       ["西陣", "西陣"],
       ["ニューギン", "ニューギン"],
       ["PーCUBE", "PーCUBE"],
       ["ビスティ", "ビスティ"],
       ["藤商事", "藤商事"],
       ["平和", "平和"],
       ["ベルコ", "ベルコ"],
       ["まさむら", "まさむら"],
       ["マルホン", "マルホン"],
       ["ミズホ", "ミズホ"],
       ["メーシー", "メーシー"],
       ["メーシー販売", "メーシー販売"],
       ["その他", "その他"]
       ] %>
    </div>
  </div>
  <br>
  <div class="row">
    <div class="large-12 columns form-width width100">
     <%= f.label :機種名〔正式名称をご記入ください。〕 %>
        <%= f.text_field :name, class: 'form-control' %>
    </div>
  </div>
  <br>
  <div class="row">
    <div class="large-12 columns">
     <%= f.label :種別 %>
     <br>
        <%= f.radio_button :type_machine, "本体" %> 本体 </label>
        <%= f.radio_button :type_machine, "セル" %> セル </label>
    </div>
  </div>
  <br>
  <div class="row">
    <div class="large-12 columns">
     <%= f.label :数量〔半角英数字〕 %>
        <%= f.text_field :number, class: 'form-control' %>
    </div>
  </div>
  <br>
  <div class="row">
    <div class="large-12 columns">
     <%= f.label :売却価格〔半角英数字〕 %>
        <%= f.text_field :price, class: 'form-control' %>
      <small class="red1">※１台あたりの価格ではございません。
      <br>上記でご入力いただいた数量の
      トータルの価格をご記入ください。</small>
    </div>
  </div>
  <br>
  <div class="row">
    <div class="large-12 columns">
     <%= f.label :前設置場所 %>
     <%= f.text_field :place, class: 'form-control' %>
       <%#= f.collection_select :place, JpPrefecture::Prefecture.all, :place, :place %>
    </div>
  </div>
  <br>
  <div class="row">
    <div class="large-12 columns">
     <%= f.label :撤去日 %>
        <%= f.radio_button :removal_date, "本体" %> 本体 </label>
        <%#= f.text_field :removal_date_year, class: 'form-control' %>
        <%#= f.text_field :removal_date_month, class: 'form-control' %>
        <%#= f.text_field :removal_date_day, class: 'form-control' %>
    </div>
  </div>
  <br>
  <div class="row">
    <div class="large-12 columns">
     <%= f.label :残債 %>
        <%= f.text_field :remnant, class: 'form-control' %>
    </div>
  </div>
  <br>
  
  
  <div class="row">
    <div class="large-12 columns">
     <%= f.label :使用次 %>
        <%= f.text_field :stage, class: 'form-control' %>
    </div>
  </div>
  <br>
  <div class="row">
    <div class="large-12 columns">
     <%= f.label :状態 %>
        <%= f.text_field :condition, class: 'form-control' %>
    </div>
  </div>
  <br>
  <div class="row">
    <div class="large-12 columns width100">
     <%= f.label :備考 %>
        <%= f.text_area :remarks, class: 'form-control' %>
    </div>
  </div>
  <br>
  
  
  
  
<!-- -- -->
    <%= f.submit "登録する", class: "btn btn-primary" %>
<!-- -- -->
<% end %>
<!-- -- -->
</div>
</div>



